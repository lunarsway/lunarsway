<h1>New lunar_release</h1>

<%= error_messages_for :lunar_release %>

<% form_for(@lunar_release) do |f| %>
  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <b>Body</b><br />
    <%= f.text_area :body %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', lunar_releases_path %>
